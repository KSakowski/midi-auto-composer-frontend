    "@vueuse/sound": "^2.0.1",
    "axios": "^1.6.0",
    "pinia": "^2.1.7",
    "primeflex": "^4.0.0",
    "primevue": "4.3.1",
    "sortablejs": "^1.15.0",
    "vite-svg-loader": "^5.1.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
